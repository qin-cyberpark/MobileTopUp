@using MobileTopUp.Models;
@{ 
    ViewBag.Title = "Your Voucher";
}
<div class="container">
    <div class="jumbotron" style="text-align:center;margin-top:15px;">
        @foreach (Voucher v in ViewBag.Vouchers)
        {
            <h4>S/N @v.Number</h4>
            <div><img src="data:image;base64,@System.Convert.ToBase64String(v.Image)" style="width:100%"/></div>
        }
        <a href="@Url.Content("~")"><button type="button" class="btn">Back</button></a>
    </div>
</div>
