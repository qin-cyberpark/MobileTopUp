@using MobileTopUp.Models;
<div class="container">
    <div class="jumbotron" style="text-align:center">
        <h4>Voucher</h4>
        @foreach (Voucher v in ViewBag.Vouchers)
        {
            <p><img src="data:image;base64,@System.Convert.ToBase64String(v.Image)"/></p>
        }
        <a href="/wx/topup/"><button type="button" class="btn">Back</button></a>
    </div>
</div>
